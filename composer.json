{
    "name": "spatie/laravel-tags",
    "description": "Add tags and taggable behaviour to your Laravel app",
    "keywords": [
        "spatie",
        "laravel-tags"
    ],
    "homepage": "https://github.com/spatie/laravel-tags",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "laravel/framework": "~5.5.0|~5.6.0|~5.7.0",
        "spatie/eloquent-sortable": "^3.0",
        "spatie/laravel-translatable": "^2.0|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2|^7.0",
        "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Tags\\": "src"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Spatie\\Tags\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Tags\\TagsServiceProvider"
            ]
        }
    }
}
